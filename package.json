{
  "name": "rock-paper-scissors",
  "version": "0.0.1",
  "description": "Rock Paper Scissors",
  "main": "src/app.js",
  "dependencies": {
    "flux": "^2.0.0",
    "radium": "^0.16.4",
    "react": "^0.14.0",
    "react-dom": "^0.14.6",
    "underscore": "^1.7.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "~6.2.0",
    "envify": "~3.0.0",
    "mcfly": "0.0.2",
    "muicss": "^0.3.0",
    "react": "^0.14.0",
    "reactify": "^0.15",
    "watchify": "~2.1.0"
  },
  "scripts": {
    "start": "watchify -o src/bundle.js -v -d . -t [ babelify --presets [ es2015 react ] ]",
    "watch": "watchify -o src/bundle.js -v -d . -t [ babelify --presets [ es2015 react ] ]",
    "build": "browserify -o src/bundle.js -v -d . -t [ babelify --presets [ es2015 react ] ] && uglifyjs src/bundle.js -mc > src/bundle.min.js"
  },
  "browserify": {
    "transform": [
      "reactify",
      "envify"
    ]
  }
}
